@{
    ViewBag.Title = "viewUsers";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

@model IEnumerable <MvcApplication2.Models.tourist>
<table class="table table-striped table-bordered table-hover table-condensed">
    <thead>
      <tr>
        <th>User Name</th>
        <th>Password</th>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Gender</th>
          <th>CNIC</th>
          <th>Contact</th>
      </tr>
    </thead>
           <tbody>
        @foreach( var item in Model)
        {
            <tr>
                <td>
                @item.username
                   
                </td>
                <td>
                @item.password
                   
                </td>
                  <td>
                @item.fname
                   
                </td>
                  <td>
                @item.lname
                   
                </td>
                  <td>
                @item.gender
                   
                </td>
                  <td>
                @item.cnic
                   
                </td>
                  <td>
                @item.contact
                   
                </td>
            </tr>
        }
               </tbody>

     </table>
